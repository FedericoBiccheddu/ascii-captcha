{
    "name": "federicobiccheddu/ascii-captcha",
    "description": "ASCII Captcha permit you to provide a simple ASCII captcha instead of images.",
    "authors": [
        {
            "name": "Federico Biccheddu",
            "email": "info@federicobiccheddu.it",
            "homepage": "http://federicobiccheddu.it",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*",
        "mockery/mockery": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "FedericoBiccheddu\\AsciiCaptcha\\": "src"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ],
        "psr-4": {
            "FedericoBiccheddu\\AsciiCaptcha\\Test\\": "tests"
        }
    },
    "minimum-stability": "stable"
}
